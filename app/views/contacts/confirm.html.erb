<div class="container">
  <div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
<h3>以下の内容で、送信する。</h3>
<%= form_for(@contacts) do |f| %>
<%= f.hidden_field :name %>
<%= f.hidden_field :email %>
<%= f.hidden_field :content %>
  <p>名前:<%= @contacts.name %></p>
  <p>Email:<%= @contacts.content %></p>
  <p>内容:<%= @contacts.content %></p>
  <%= f.submit "送信する" ,class: 'btn btn-primary btn-block'%>
<% end %>
<%= form_for(@contacts, url: new_contact_path, method: 'get') do |f| %>
  <%= f.hidden_field :name, class: 'form-control' %>
<%= f.hidden_field :email, class: 'form-control' %>
<%= f.hidden_field :content, class: 'form-control' %>
  <%= f.submit "もどる" ,name: 'back',class: 'btn btn-primary btn-block'%>
<% end %>
</div>
</div>
